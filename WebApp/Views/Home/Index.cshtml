@model IEnumerable<Domain.Model.Car>
@{
    ViewBag.Title = "Home Page";
}

<div class="row">
    <div class="col-md-6">
        <h1>Cars</h1>
        <table class="table">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Model</th>
                    <th>Make</th>
                </tr>
            </thead>
            <tbody>
            @foreach (var car in Model) {
                <tr>
                    <td>@car.Id</td>
                    <td>@car.Model</td>
                    <td>@car.Make</td>
                </tr>
            }
            </tbody>
        </table>
    </div>
</div>